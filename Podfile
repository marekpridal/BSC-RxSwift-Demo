# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'BSC' do

  use_frameworks!

  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Swinject'
  pod 'SwinjectAutoregistration'
  pod 'SwinjectStoryboard', :git => 'https://github.com/Swinject/SwinjectStoryboard.git', :branch => 'master'

end

target 'BSCTests' do

  use_frameworks!

  pod 'RxSwift'
  pod 'RxRelay'

end